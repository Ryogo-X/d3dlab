<UserControl x:Class="D3DLab.Viewer.Presentation.TDI.SceneHwndHostUCTab"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:D3DLab.Viewer.Presentation.TDI"
             xmlns:controls="clr-namespace:D3DLab.Wpf.Render.Overlay;assembly=D3DLab.Wpf.Render"
             xmlns:render="clr-namespace:D3DLab.Render;assembly=D3DLab.Wpf.Render"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>

        <render:Host x:Name="hwndHost"/>

        <controls:OverlayPanel>
            <Grid Background="#01ffffff" x:Name="overlay" />
        </controls:OverlayPanel>
        
    </Grid>
</UserControl>
